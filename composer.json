{
    "name": "gulch/gulchuk-website",
    "description": "Personal website gulchuk.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Volodymyr Gulchuk",
            "email": "volodymyr@gulchuk.com",
            "role": "Developer"
        }
    ],
    "require": {
        "filp/whoops": "^2.0",
        "gulch/assets": "^0.2",
        "gulch/minify": "^0.1",
        "hollodotme/fast-cgi-client": "^2.4",
        "illuminate/database": "^5.5",
        "intervention/image": "^2.3",
        "league/container": "^2.0",
        "league/plates": "^3.0",
        "league/route": "^3.0",
        "monolog/monolog": "^1.23",
        "samdark/sitemap": "^2.0",
        "suin/php-rss-writer": "^1.5",
        "symfony/finder": "^3.3",
        "vlucas/phpdotenv": "^2.2",
        "zendframework/zend-diactoros": "^1.3",
        "zendframework/zend-inputfilter": "^2.7",
        "zendframework/zend-servicemanager": "^3.3"
    },
    "require-dev": {
        "robmorgan/phinx": "^0.9",
        "fzaninotto/faker": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/functions.php"
        ],
        "exclude-from-classmap": [
            "/Tests/", 
            "/test/",
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
